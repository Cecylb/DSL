//CONNECTIONS BLOCK

teXconn1(linetype, index1, objName1, port1)::=<< \draw [<linetype>] (<objName1>\m<index1>.<port1>) >> //START POINT
teXconnC(objName, index, port, x, y)::=<<-- ($ (<objName>\m<index>.<port>) + (<x>,<y>) $)>> //OPTIONAL FOR THE LINE ANGLES
teXconn2(index2, objName2, port2)::=<< --(<objName2>\m<index2>.<port2>);


>> //END POINT (CONNECTION)
teXconnIO(eorw, port)::=<<-- +(1,0) node [anchor=<eorw>] {<port>};


>> //END POINT (IO)


